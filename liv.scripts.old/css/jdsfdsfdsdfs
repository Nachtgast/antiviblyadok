			<span v-if="user.info.country_flag" v-html="user.info.country_flag"></span>

			<span class="city" v-if="user.info.city" v-show="user.info.city"><a target="_blank" v-bind:href="user.info.city_link">${user.info.city}</a></span>
			<span v-else v-html="user.info.country"></span>



			<span v-if="user.info.extended && user.info.extended.sex">
				<span data-title="Женский пол" v-if="user.info.extended.sex == 'f'" class=""><i class="em em-girl" aria-role="presentation" aria-label="GIRL"></i></span>
				<span data-title="Мужской пол" v-else-if="user.info.extended.sex == 'm'"><i class="em em-face_with_cowboy_hat" aria-role="presentation" aria-label="FACE WITH COWBOY HAT"></i></span>
				<span data-title="Транс" v-else-if="user.info.extended.sex == 't'"><i class="em em-female_elf" aria-role="presentation" aria-label=""></i></span>
			</span>

			<strong v-show="user.info.age" data-title="Возраст">${user.info.age}</strong>



		</div>

		<div class="user-descr mb-2" v-show="(user.info.extended && user.info.extended.text)"><i>${(user.info.extended && user.info.extended.text) ? user.info.extended.text : ""}</i></div>

