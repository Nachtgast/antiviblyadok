<script>

	(function(){

	    try {

            var ikey = "__jserrs2";
            var stor = typeof window.localStorage === "object" && typeof window.localStorage.getItem === "function";
		    var limitStore = 60 * 15; //sec
            if(stor) {
                window.localStorage.removeItem("__jserrs");
            }

            var getItems = function () {
                var items = [];
                try {
                    items = window.localStorage.getItem(ikey);
                    if (items) {
                        items = JSON.parse(items);
                    }

                } catch (e) {
                    console.error("error get items", e);
                }

                return items instanceof Array ? items : [];
            };

            window.onerror = function (err, file, line, col, error) {

                var sign = err + " " + file + ":" + line;

                var hasSend = true;
                var items = [];

                var ts = (new Date()).getTime();

                if (stor) {

                    var delta;
                    items = getItems();
                    var i = items.length;
                    while (i--) {
                        var item = items[i];
                        delta = (ts - item.ts) / 1000;

                        if (item.sign === sign) {
                            if(delta < limitStore) {
                                hasSend = false;
                            }
                        }

                        if (delta > limitStore) {
                            items.splice(i, 1);
                        }
                    }

                }


                if (hasSend) {

                    if(typeof XMLHttpRequest === "function") {
                        var xhr = new XMLHttpRequest();
                        xhr.open("POST", "https://livacha.com/jsError", true);

                        var json = JSON.stringify({
                            sign: sign,
                            error: err,
                            file: file,
                            line: line,
                            uid: window.__app_settings && window.__app_settings.uid ? window.__app_settings.uid : "",
                            col: col,
                            href: window.location.href,
                            stack: error && typeof error === "object" && error.stack ? error.stack : "",
                            allErrors: items,
                            page: document.documentElement.outerHTML || document.documentElement.innerHTML
                        });

                        xhr.send(json);
                    }

                    if (stor) {

                        items.push({
                            ts: ts,
                            sign: sign
                        });

                        window.localStorage.setItem(ikey, JSON.stringify(items));
                    }
                }
            }
        }catch(e){
	        console.error(e);
	    }
	}());



</script>
